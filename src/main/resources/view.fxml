    <Label fx:id="inertiaValueLabel" layoutX="471.0" layoutY="30.0" text="0.0"/>
    <Label fx:id="socialValueLabel" layoutX="373.0" layoutY="85.0" text="0.0"/>
    <Label fx:id="cognitiveValueLabel" layoutX="542.0" layoutY="85.0" text="0.0"/>
    <Button fx:id="clearButton" layoutX="138.0" layoutY="544.0" mnemonicParsing="false" onAction="#clearButtonOnAction"
            prefHeight="31.0" prefWidth="80.0" text="Clear"/>
</AnchorPane>
