    <Slider fx:id="socialSlider" layoutX="265.0" layoutY="109.0"/>
    <Slider fx:id="inertiaSlider" layoutX="362.0" layoutY="56.0"/>
    <Slider fx:id="cognitiveSlider" layoutX="432.0" layoutY="110.0"/>
    <TextField fx:id="epochsTextField" layoutX="115.0" layoutY="95.0" prefHeight="31.0" prefWidth="90.0"/>
    <TextField fx:id="particlesTextField" layoutX="115.0" layoutY="153.0" prefHeight="31.0" prefWidth="90.0"/>
    <TextArea fx:id="logTextArea" layoutX="34.0" layoutY="206.0" prefHeight="316.0" prefWidth="543.0"/>
    <Label layoutX="39.0" layoutY="99.0" text="Epochs"/>
    <Label layoutX="39.0" layoutY="158.0" text="Particles"/>
    <Label layoutX="370.0" layoutY="32.0" text="Inertia"/>
